<% content_for 'sub_menu' do %>
	<ul id="sub_menu">
		<li><%= link_to 'All Nodes', nodes_path %></li>
	</ul>
<% end %>

<h2>Edit Node</h2>

<% form_for @node, :url => node_path do |f| %>
  <%= f.error_messages %>
	<table class='table_form'>
  	<%= render :partial => "form", :object => f %>
	</table>
  <%= f.submit "Update" %>
<% end %>