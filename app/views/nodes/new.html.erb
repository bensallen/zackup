<% content_for 'sub_menu' do %>
	<ul id="sub_menu">
		<li><%= link_to 'All Nodes', nodes_path %></li>
	</ul>
<% end %>

<h2>Create New Node</h2>

<% form_for @node, :url => nodes_path do |f| %>
  <%= f.error_messages %>
	<table class='table_form'>
  	<%= render :partial => "form", :object => f %>
	</table>
  <%= f.submit "Create" %>
<% end %>