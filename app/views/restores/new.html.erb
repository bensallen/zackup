<% content_for 'sub_menu' do %>
	<ul id="sub_menu">
			<li><%= link_to 'View Host', host_path(@host.id) %></li>
	</ul>
<% end %>

<h2>Create New Store Job</h2>
<% form_for [@host, @restore] do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.collection_select(:schedule_id, @schedules, :id, :name)%>
	<%= submit_tag 'Next' %>
<% end %>