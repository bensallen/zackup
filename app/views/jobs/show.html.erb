<% content_for 'sub_menu' do %>
	<ul id="sub_menu">
			<li><%= link_to 'All Jobs', jobs_path %></li>
	</ul>
<% end %>

<h2>Job ID: <%= @job.id %> </h2>
<table class='table_form'>
	<tr>
		<th class='labelform'>Schedule</th>
  	<th><%=h @job.schedule.name if @job.schedule %></th>
	</tr>
	<tr>
		<th class='labelform'>Host</th>
  	<th><%=h @job.host.name if @job.host %></th>
	</tr>
</table>